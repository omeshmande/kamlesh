        stage('pull') { 
            steps {
                echo "pull successful"
            }
        }
        stage('build') { 
            steps {
                echo "build successful"
            }
        }
        stage('test') { 
            steps {
                echo "test successful"
            }
        }
        stage('deploy') {
        steps {
        echo "deploy successful"
        }
        }
    }
}
